{
  "name": "advisor-app-build",
  "source": {
    "type": "GITHUB",
    "location": "https://github.com/asiamahy2g/adviseme.git",
    "buildspec": "buildspec.yml"
  },
  "artifacts": {
    "type": "CODEPIPELINE"
  },
  "environment": {
    "type": "LINUX_CONTAINER",
    "image": "aws/codebuild/amazonlinux2-x86_64-standard:3.0",
    "computeType": "BUILD_GENERAL1_MEDIUM",
    "privilegedMode": true,
    "environmentVariables": [
      {
        "name": "AWS_DEFAULT_REGION",
        "value": "us-east-1"
      },
      {
        "name": "AWS_ACCOUNT_ID",
        "value": "540738452774"
      },
      {
        "name": "IMAGE_REPO_NAME",
        "value": "advisor-app"
      },
      {
        "name": "IMAGE_TAG",
        "value": "latest"
      }
    ]
  },
  "serviceRole": "arn:aws:iam::540738452774:role/CodeBuildServiceRole"
}
